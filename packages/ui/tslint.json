{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "never"],
        "quotemark": [true, "single", "jsx-double"],
        "interface-name": false,
        "object-literal-sort-keys": false,
        "ordered-imports": [
            true,
            {
                "import-sources-order": "any",
                "named-imports-order": "any"
            }
        ]
    },
    "rulesDirectory": []
}